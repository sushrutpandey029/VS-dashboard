												</td>
												<td>{{this.email}}</td>
												<td>{{this.phone}}</td>
                                                <form action="http://localhost:3000/patient_dashboard/{{this._id}}">
												<td><button type="submit" class="btn btn-primary btn-primary-one float-right">View</button></td>
                                                </form>
											</tr>
										
										</tbody>
									</table>
								</div>
                                {{/each}}
